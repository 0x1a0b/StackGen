<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="context1">
    <plugin type="io.starter.ignite.generator.MyBatisIgnitePluginAdapter" />
    <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
    <!-- TODO <plugin type="io.starter.ignite.generator.MyBatisIgnitePluginAdapter"> 
			<property name="annotationClass" value="io.starter.ignite.security.securefield.SecureField" 
			/> <property name="annotationString" value="@SecureField" /> </plugin> -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://${RDS_HOSTNAME}:3306/${RDS_DB_NAME}?autoReconnect=true" userId="${RDS_USERNAME}" password="${RDS_PASSWORD}" />
    <!-- Model Class -->
    <javaModelGenerator targetPackage="io.starter.${ARTIFACT_ID}.model.dao" targetProject="${MYBATIS_MAIN}main/java">
      <property name="enableSubPackages" value="false" />
      <property name="trimStrings" value="true" />
      <property name="rootClass" value="io.starter.ignite.model.DataModelBase" />
      <property name="type" value="io.starter.ignite.generator.MyBatisIgnitePluginAdapter" />
      <property name="addRemarkComments" value="true" />
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="io.starter.${ARTIFACT_ID}.sqlmaps" targetProject="${MYBATIS_MAIN}main/resources" />
    <javaClientGenerator targetPackage="io.starter.${ARTIFACT_ID}.model.dao" targetProject="${MYBATIS_MAIN}main/java" type="XMLMAPPER" />
    <table schema="${ARTIFACT_ID}" tableName="PLACEHOLDER_NODE" />
    <table schema="starter" tableName="starter$order">
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    <table schema="starter" tableName="starter$model_api_response">
